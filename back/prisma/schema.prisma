generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model smoking_Log {
  user_id          String    @id
  user_name        String
  created_at       DateTime? @db.Date
  smoke_totalCount Int?      @default(0)
  smoke_type       String    @default("紙たばこ")
  smoke_price      Int?      @default(550) @db.SmallInt
  smoke_todayCount Int?      @default(0)
}

model items {
  item_id          Int    @id @default(autoincrement())
  item_name        String
  item_price       Int
}
